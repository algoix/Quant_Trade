OptimizerSetEngine("cmae"); // we can use "spso" or "trib" here

_SECTION_BEGIN("Trading Set up");
//Filename = _DEFAULT_NAME();
SubmitOrders = True; 
Tracing = False;    
MarketON=093000;
US_Trade_On=TimeNum() >= 094000 AND TimeNum() <=154500;
printf("US_Trade_On:"+"\t"+US_Trade_On+"\n");
ABName="SPY";//IBName =Name();//getfndata("Alias");
Bars_so_far_today = 1 + BarsSince( Day() != Ref(Day(), -1));
printf("Bars already for today:"+"\t"+Bars_so_far_today+"\n");
_SECTION_END();



_SECTION_BEGIN("SPY-Variables");
SetForeign(ABName);
O_SPY = O;
H_SPY = H;
L_SPY = L;
C_SPY = C;
v_SPY = IIf(V==0,1,Volume);
RestorePriceArrays();

// Useful to find state of market
C_YDay_SPY = TimeFrameGetPrice("C_SPY", inDaily, -1); // yesterdays close https://www.amibroker.com/guide/h_timeframe.html
H_YDay_SPY = TimeFrameGetPrice("H_SPY", inDaily, -1);
L_YDay_SPY = TimeFrameGetPrice("L_SPY", inDaily, -1);
C_5Min_SPY=TimeFrameGetPrice("C_SPY", in5minute, -1);// TimeFrameExpand( ma5_13, in5Minute)
L_5Min_SPY=TimeFrameGetPrice("L_SPY",in5minute, -1);// TimeFrameExpand( ma5_13, in5Minute)
H_5min_SPY=TimeFrameGetPrice("H_SPY",in5minute,-1);
O_5min_SPY=TimeFrameGetPrice("O_SPY",in5minute,-1);

//Useful for normalization
SP_SPY=ValueWhen(Bars_so_far_today==600,O);
printf("Start price %g\n",SP_SPY);

//Useful to find active market
price=(Ref(H_SPY,-1)+Ref(L_SPY,-1)+Ref(C_SPY,-1))/3;
R1=price*2-Ref(L_SPY,-1);
R2=price+(Ref(H_SPY,-1)-Ref(L_SPY,-1));
S1=price*2-Ref(H_SPY,-1);
S2=price-(Ref(H_SPY,-1)-Ref(L_SPY,-1));

// SENTIMENT
opening_jump=ValueWhen(Bars_so_far_today==1,O)-C_YDay_SPY;
present_jump=O_SPY-C_YDay_SPY;
sentiment=present_jump/opening_jump;
//sentiment = (sentiment - MA(sentiment,60 ) ) / StDev(sentiment,60);
printf("sentiment %g\n",sentiment);
//NEED to add LSTM/NN twitter input
// polarized sentiment are  mean reverting. Price level at any market state will break up or down based on this ! [20,-20]


// VELOCITY
// velocity per 5m above 0.25 is high speed.  
velocity_5m=(O_SPY-Ref(O_SPY,-60))/0.25;
velocity=Sum(IIf(velocity_5m>1,1,IIf(velocity_5m<-1,-1,0)),60);
printf("velocity %g\n",velocity);
// velocity increases above 20 can lead to reversion
rev_p=sentiment>5 OR velocity>10;
rev_n=sentiment<-5 OR velocity<-10;
Bars_since_rev_p = 1 + BarsSince(rev_p==1);
Bars_since_rev_p =IIf(Bars_since_rev_p>30,0,Bars_since_rev_p);
Bars_since_rev_n = 1 + BarsSince(rev_n==1);
Bars_since_rev_n =IIf(Bars_since_rev_n>30,0,Bars_since_rev_n);

//printf("rev_p %g\n",rev_p);
//printf("rev_n %g\n",rev_n);
price_diff_since_rev_p=IIf(Bars_since_rev_p>0,sum(O_SPY-Ref(O_SPY,-rev_p),12),0);
price_diff_since_rev_n=IIf(Bars_since_rev_n>0,sum(O_SPY-Ref(O_SPY,-rev_n),12),0);
printf("price_diff_since_rev_p %g\n",price_diff_since_rev_p);
printf("price_diff_since_rev_n %g\n",price_diff_since_rev_n);

//Normalized data==ND. ND >1 == buy if ND<1 sell short
// see D
ND=O_SPY/SP_SPY;
printf("Market UP/DOWN %g\n",ND);

//ACTIVE market. see previous version SPY_1403
ACTV=v_SPY>50 AND v_SPY>MA(v_SPY,60)*0.75;

//### mean reversion indicators for MR
//VWAP spread
totalVolume = Sum(Ref(v_SPY,-1),12);
VWAP = Sum (price *Ref(v_SPY,-1),12) /totalVolume;
//# price and VWAP difference
spread=O_SPY-VWAP;
spread=Sum(spread,60);
spread=(spread - MA(spread,60 ) ) / StDev(spread,60);

// Regression spread
OLS_spread=Sum(O_SPY-(LinRegIntercept(O_SPY,60)+LinRegSlope(O_SPY, 60 )*O_SPY),60);
OLS_spread=(OLS_spread - MA(OLS_spread,60 ) ) / StDev(OLS_spread,60);

//## UT tradeable upper line and DT is tradable down line
UT=(HHV(H_SPY,60) + HHV(H_SPY-L_SPY,60)*0.62);
DT=(LLV(L_SPY,60) + LLV(H_SPY-L_SPY,60)*0.62);

// Distance
D=O_SPY-SP_SPY;
D_zs=(D-MA(D,60))/StDev(D,60);

// Buy and Sell short band identification. Band=B
B=IIf(O_SPY>UT,1,IIf(O_SPY<DT,-1,0));
// B==1 for sell short and B==-1 for buy at state 2 and 3 ( mean reversion). But for 1 and 4, b==1 for buy and -1 for sell short ( trend). 

m_rev_p=spread>2 OR OLS_spread>2 OR D_zs>2 OR B==1;
Bars_since_m_rev_p = 1 + BarsSince(m_rev_p==1);
Bars_since_m_rev_p =IIf(Bars_since_m_rev_p>12,0,Bars_since_m_rev_p);

m_rev_n=spread<-2 OR OLS_spread<-2 OR D_zs<-2 OR B==-1;
Bars_since_m_rev_n = 1 + BarsSince(m_rev_n==1);
Bars_since_m_rev_n =IIf(Bars_since_m_rev_n>12,0,Bars_since_m_rev_n);
price_diff_since_m_rev_p=IIf(Bars_since_m_rev_p>0,sum(O_SPY-Ref(O_SPY,-m_rev_p),12),0);
price_diff_since_m_rev_n=IIf(Bars_since_m_rev_n>0,sum(O_SPY-Ref(O_SPY,-m_rev_n),12),0);
printf("price_diff_since_m_rev_p %g\n",price_diff_since_m_rev_p);
printf("price_diff_since_m_rev_n %g\n",price_diff_since_m_rev_n);

_SECTION_END();



_SECTION_BEGIN("OPTIMIZATION"); //##//SPY//
//inv_SPY=Optimize("investment",45000,30000,60000,1000 );
_SECTION_END();

_SECTION_BEGIN("SPY STRATEGY");//HEDGING by SPXL,SPXU,SPXS 2000K amount for 23 SPY ( approximation)

//#retracement 24%,38%,50%,62% and 124%. Not considered 


x = BarIndex() + 1;
lastx = LastValue( x ) - 60;
//
// compute linear regression coefficients
aa = LastValue( Ref( LinRegIntercept(O_SPY,60), -60 ) );
bb = LastValue( Ref( LinRegSlope(O_SPY, 60 ), - 60 ) );
//
// the equation for straight line
y = aa + bb * ( x - ( Lastx - 60 + 1 ) );
//y=aa+bb*O_SPY;



//MARKET STATE == MS. No trade at 0 state
// MS==4 is extremely up and 1 is extremely down

UL=H_YDay_SPY; 
LL=L_YDay_SPY;
NL=(UL+LL)/2;

MS=IIf(SP_SPY>UL,4,IIf(SP_SPY>NL AND SP_SPY<UL,3,IIf(SP_SPY<NL AND SP_SPY>LL,2,IIf(SP_SPY<LL,1,0))));
printf("op mkt state %g\n",MS);//
MS_pr=IIf(O_SPY>UL,4,IIf(O_SPY>NL AND O_SPY<UL,3,IIf(O_SPY<NL AND O_SPY>LL,2,IIf(O_SPY<LL,1,0))));
printf("pr mkt state %g\n",MS_pr);//
MS_change=MS_pr-MS;
printf("mkt state chg %g\n",MS_change);//
Bars_since_MS_change = 1 + BarsSince(MS_change!=0);
Bars_since_MS_change=IIf(Bars_since_MS_change>60,0,Bars_since_MS_change);
price_diff_since_MS_change=O_SPY-Ref(O_SPY,-Bars_since_MS_change);
price_diff_since_MS_change=IIf(Bars_since_MS_change>0,sum(O_SPY-Ref(O_SPY,-MS_change),12),0);

// Zone 1
	//MR
BC_MR_1= MS_pr==1 AND MS_change==0 AND price_diff_since_m_rev_n>0.01 AND price_diff_since_m_rev_p==0 AND sentiment<4 AND velocity<5;
SSC_MR_1=MS_pr==1 AND MS_change==0 AND price_diff_since_m_rev_p<-0.01 AND price_diff_since_m_rev_n==0 AND sentiment<4 AND velocity>-5;
	// Reversion
BC_R_1= MS_pr==1 AND MS_change<=0 AND B<=0 AND price_diff_since_rev_n>0.05 AND Bars_since_m_rev_p==0;
	// Momentum zone
SSC_MM_zone_1= MS_pr==1 AND MS_change<0 AND B<=0 AND price_diff_since_rev_n<-0.05 AND Bars_since_m_rev_n==0;


// Zone 2
	//MR
BC_MR_2= MS_pr==2  AND MS_change>=0 AND price_diff_since_m_rev_n>0.01 AND sentiment<4 AND velocity<5;
SSC_MR_2= MS_pr==2  AND MS_change>=0 AND price_diff_since_m_rev_p<-0.01 AND sentiment<4 AND velocity>-5;
	// Reversion
SSC_R_2= MS_pr==2 AND MS_change>0 AND B>=0 AND price_diff_since_rev_p<-0.05 AND Bars_since_m_rev_n==0;
BC_R_2= MS_pr==2 AND MS_change<0  AND B<=0 AND price_diff_since_rev_n>0.05 AND Bars_since_m_rev_p==0;
	// Momentum zone
BC_MM_zone_2=MS_pr==2 AND MS_change>0 AND B==1 AND  price_diff_since_rev_p>0.05 AND Bars_since_m_rev_n==0;
SSC_MM_zone_2=MS_pr==2 AND MS_change<0 AND B==-1 AND price_diff_since_rev_n<-0.05 AND Bars_since_m_rev_p==0;

// Zone 3
	//MR
BC_MR_3= MS_pr==3 AND MS_change>=0 AND price_diff_since_m_rev_n>0.01 AND sentiment<4 AND velocity<5;
SSC_MR_3=MS_pr==3 AND MS_change>=0 AND price_diff_since_m_rev_p<-0.01 AND sentiment<4 AND velocity>-5;
	// Reversion
SSC_R_3= MS_pr==3 AND MS_change>0 AND B>=0 AND price_diff_since_rev_p<-0.05 AND Bars_since_m_rev_n==0;
BC_R_3= MS_pr==3 AND MS_change<0 AND B<=0 AND price_diff_since_rev_n>0.05 AND Bars_since_m_rev_p==0;
	// Momentum zone
BC_MM_zone_3=MS_pr==3 AND MS_change>0 AND B==1 AND  price_diff_since_rev_p>0.05 AND Bars_since_m_rev_n==0;
SSC_MM_zone_3=MS_pr==3 AND MS_change<0 AND B==-1 AND price_diff_since_rev_n<-0.05 AND Bars_since_m_rev_p==0;

// Zone 4
	//MR
BC_MR_4= MS_pr==4 AND MS_change>=0 AND price_diff_since_m_rev_n>0.01 AND sentiment<4 AND velocity<5;
SSC_MR_4=MS_pr==4 AND MS_change>=0 AND price_diff_since_m_rev_p<-0.01 AND sentiment<4 AND velocity>-5;
	// Reversion
SSC_R_4 = MS_pr==4 AND MS_change>0 AND B==-1 AND price_diff_since_rev_p<-0.05 AND Bars_since_m_rev_n==0;
	// Momentum zone
BC_MM_zone_4=MS_pr==4 AND MS_change>0 AND B==1 AND  price_diff_since_rev_p>0.05 AND Bars_since_m_rev_n==0;


BC=BC_MR_1 OR BC_R_1 OR BC_MR_2 OR BC_R_2 OR BC_MM_zone_2 OR BC_MR_3 OR BC_R_3 OR BC_MM_zone_3 OR BC_MR_4 OR BC_MM_zone_4;
SSC=SSC_MR_1 OR SSC_MM_zone_1 OR SSC_MR_2 OR SSC_R_2 OR SSC_MM_zone_2 OR SSC_MR_3 OR SSC_R_3 OR SSC_MM_zone_3 OR SSC_MR_4 OR SSC_R_4; 

BC=BC AND Cross(O_SPY,MA(O_SPY,5)) AND ACTV;
SSC= SSC AND Cross(MA(O_SPY,5),O_SPY) AND ACTV;
Bars_since_BC = 1 + BarsSince(BC==1);
Bars_since_BC=IIf(Bars_since_BC>12,0,Bars_since_BC);
price_diff_since_BC=IIf(Bars_since_BC>0,sum(O_SPY-Ref(O_SPY,-Bars_since_BC),12),0);
printf("price_diff_since_BC %g\n",price_diff_since_BC);
Bars_since_SSC = 1 + BarsSince(SSC==1);
Bars_since_SSC=IIf(Bars_since_SSC>12,0,Bars_since_SSC);
price_diff_since_SSC=IIf(Bars_since_SSC>0,sum(O_SPY-Ref(O_SPY,-Bars_since_SSC),12),0);
printf("price_diff_since_SSC %g\n",price_diff_since_SSC);

SC=(rev_p==1 OR B==1) AND m_rev_n==0 AND Bars_since_BC==0 AND Cross(R1,O_SPY);
CC=(rev_n==1 OR B==1) AND m_rev_p==0 AND Bars_since_SSC==0 AND Cross(O_SPY,S1);

OSZ=750;//Param("order size",100,50,200);//LastValue(inv_SPY/C_SPY);
			//ACT==signal
			// Buy Classification(BC): (1) ACTV_B==1 (2) O_SPY<DT (3) DT>LL and DT<NL (4) Angle of DT not stiff
			// Short Sell Classification: (1) ACTV_SS==1 (2) O_SPY>UT (3) UT>NL and UT<UL (4) angle of UT not stiff
			// (1)DT<LL and (2) stiff angle leads to SPXS buy(2000). Assumed long position of SPY (OSZ)
			// (2) UT>UL and (2) stiff angle leads to SPXL buy(100). Assumed short position of SPY (-OSZ)
_SECTION_END();  


